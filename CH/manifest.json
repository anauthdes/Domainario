{
  "manifest_version": 2,

  "name": "Domainario",
  "description": "Simple Extension for replacing the ends of domains with a simple shortcut with exceptions. ",
  "version": "1.0",
   "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "browser_action": {
    "default_icon": "Domainario.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "web_accessible_resources": [
    "Domainario.png"
  ],
  "background":{
    "scripts": ["backgroundcheck.js"]
  }
}
